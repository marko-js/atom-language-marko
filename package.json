{
  "name": "language-marko",
  "description": "Marko Tooling for Atom",
  "version": "3.2.8",
  "bugs": "https://github.com/marko-js/language-server/issues/new?template=Bug_report.md",
  "consumedServices": {
    "console": {
      "versions": {
        "0.1.0": "consumeConsole"
      }
    },
    "datatip": {
      "versions": {
        "0.1.0": "consumeDatatipService"
      }
    },
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinterV2"
      }
    }
  },
  "dependencies": {
    "@marko/language-server": "^0.12.17",
    "atom-languageclient": "^1.15.0",
    "atom-package-deps": "^8.0.0"
  },
  "engines": {
    "atom": ">=1.21.0"
  },
  "enhancedScopes": [
    "text.marko"
  ],
  "homepage": "https://github.com/marko-js/language-server/tree/master/clients/atom/README.md",
  "keywords": [
    "autocomplete",
    "marko",
    "syntax",
    "tags",
    "template",
    "templating"
  ],
  "license": "MIT",
  "main": "./src/index.js",
  "package-deps": [
    "atom-ide-ui"
  ],
  "private": true,
  "providedServices": {
    "hyperclick": {
      "versions": {
        "0.1.0": "provideHyperclick"
      }
    },
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provideAutocomplete"
      }
    },
    "code-format.range": {
      "versions": {
        "0.1.0": "provideCodeFormat"
      }
    },
    "definitions": {
      "versions": {
        "0.1.0": "provideDefinitions"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/marko-js/atom-language-marko/"
  }
}
