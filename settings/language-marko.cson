
".text.marko":
  editor:
    commentStart: "<!-- "
    commentEnd: " -->"
    increaseIndentPattern: "<(?!\\?|(?:area|base|br|col|frame|hr|html|img|input|link|meta|param|state)\\b|[^>]*\\/>)([-_\\.A-Za-z0-9]+)(?=\\s|>)\\b[^>]*>(?!.*<\\/\\1>)|<!--(?!.*-->)|\\{[^}\"']*$"
    decreaseIndentPattern: "^\\s*(<\\/(?!html)[-_\\.A-Za-z0-9]+\\b[^>]*>|-->|\\})"